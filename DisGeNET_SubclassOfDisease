Select ?disease ?do ?doname ?mesh ?meshname
Where {
?disease rdf:type ncit:C7057 ; rdfs:subClassOf ?do .
?do rdfs:label ?doname .
?disease sio:SIO_000095 ?mesh .
?mesh rdfs:label ?meshname
}

Select ?disease ?do ?mesh ?doname
Where {
?disease rdf:type ncit:C7057 ; rdfs:subClassOf ?do .
?do rdfs:label ?doname .
?disease sio:SIO_000095 ?mesh 
}
